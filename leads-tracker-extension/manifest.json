{
    "manifest_version": 3,
    "version": "1.0",
    "name": "Leads tracker",
    "description": "Save leads and notes from any website",
    "action": {
        "default_popup": "index.html",
        "default_icon": "icon.png"
    },
    "permissions": ["activeTab", "scripting"],
    "host_permissions": ["<all_urls>"]
}
